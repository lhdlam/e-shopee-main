        "swiper": "^8.4.7",
        "vue": "^3.5.12",
        "vue-demi": "^0.12.5",
        "vue-echarts": "^6.7.3",
        "vue-i18n": "^9.14.1",
        "vue-router": "^4.4.5",
        "vue-slick-carousel": "^1.0.6",
        "vue-toasted": "^1.1.28",
